{
  "name": "wya-rollup",
  "version": "0.1.0",
  "description": "",
  "main": "dist/pluginsName.min.js",
  "files": [
    "src/**",
    "dist/**",
    "examples/**"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run serve",
    "serve": "rm -rf build/ && cross-env NODE_ENV=development rollup --config rollup.config.js --watch",
    "build": "rm -rf dist/ && cross-env NODE_ENV=production rollup --config rollup.config.js",
    "lint": "esw *.config.* ./src/",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --fix --watch",
    "all": "npm-run-all --parallel --race --continue-on-error lint:watch serve",
    "lint-staged": "lint-staged",
    "pre-commit:install": "cp .hooks/* .git/hooks/",
    "test": "jest --config ./tests/config/jest.config.js"
  },
  "author": "wya-team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wya-team/wya-rollup"
  },
  "bugs": {
    "url": "https://github.com/wya-team/wya-rollup/issues"
  },
  "dependencies": {
    "debug": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.0.4",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babelrc-rollup": "^3.0.0",
    "cross-env": "^5.1.3",
    "cssnano": "^3.10.0",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.14.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-watch": "^3.1.3",
    "fs-extra": "^5.0.0",
    "jest": "^22.0.5",
    "lint-staged": "^6.0.0",
    "npm-run-all": "^4.1.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-nested": "^3.0.0",
    "postcss-simple-vars": "^4.1.0",
    "pre-commit": "^1.2.2",
    "prompt": "^1.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.53.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-eslint": "^4.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.5",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "esw --fix",
      "git add"
    ],
    "*.md": [
      "esw --ext '.md' --fix",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ]
}
